{
  "manifest_version": 3,
  "name": "Gemini Cookie 同步器",
  "description": "自动从 gemini.google.com 获取 Cookie 并上传到本地服务器",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "https://gemini.google.com/*",
    "https://*.google.com/*",
    "http://localhost/*",
    "http://127.0.0.1/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Gemini Cookie 同步器"
  }
}
